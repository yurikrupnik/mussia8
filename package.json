{
  "name": "mussia8",
  "version": "1.0.0",
  "description": "Mussia8 base project monorepo poc for api gateway and more!",
  "main": "index.js",
  "scripts": {
    "postinstall": "lerna exec --parallel -- npm i",
    "bootstrap": "lerna bootstrap",
    "dev": "lerna run --parallel dev",
    "lint": "lerna run --parallel lint",
    "test": "lerna run --parallel test",
    "tsc": "lerna run --parallel tsc",
    "build": "lerna run --parallel build",
    "npm-publish": "bash scripts/publish-npm.sh",
    "compose:dev:up": "lerna run --parallel compose:dev:up",
    "compose:dev:down": "lerna run --parallel compose:dev:down",
    "waypoint": "lerna run --parallel waypoint",
    "styleguidist:dev": "npx styleguidist server --open",
    "styleguidist:build": "npx styleguidist build",
    "typedoc": "typedoc --tsconfig tsconfig.json",
    "docs:dev": "LOCAL=true run-p styleguidist:dev 'typedoc -- --watch'",
    "docs:build": "run-p styleguidist:build typedoc",
    "docs:dev:server": "npx http-server docs/styleguide",
    "start": "run-p docs:dev docs:dev:server",
    "dev:all": "run-p dev start",
    "deploy": "lerna run --parallel deploy",
    "prebuild-pages": "npm run docs:build",
    "build-pages": "gh-pages -d docs -a"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/mussia8.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/mussia8/issues"
  },
  "homepage": "https://yurikrupnik.github.io/mussia8/",
  "devDependencies": {
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@types/jest": "27.0.1",
    "@types/node": "15.3.0",
    "@types/react": "17.0.19",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "autoprefixer": "10.3.2",
    "css-loader": "6.2.0",
    "esbuild-loader": "2.15.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.24.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "generate-json-webpack-plugin": "2.0.0",
    "gh-pages": "3.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "lerna": "4.0.0",
    "node-sass": "6.0.1",
    "nodemon-webpack-plugin": "4.5.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "process": "0.11.10",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-styleguidist": "11.1.7",
    "rollup": "2.56.3",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-esbuild": "4.2.3",
    "rollup-plugin-postcss": "4.0.1",
    "rollup-plugin-swagger-jsdoc": "1.0.1",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "swagger-jsdoc-webpack-plugin": "1.0.0",
    "typedoc": "0.21.6",
    "typescript": "4.3.5",
    "webpack": "5.51.1",
    "webpack-cli": "4.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 4,
    "trailingComma": "none"
  }
}
